.tabBar {
  position: fixed;
  width: 100%;
  height: 138rpx;
  bottom: 0;
}

.tabBar .tabBar-item {
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  align-items: center;
  width: 35%;
  height: 100rpx;
  margin-top: 38rpx;
  background: #F8F8F8;
}

.tabBar-item .container {
  width: 66.4rpx;
  height: 100rpx;
}

.tabBar-item image {
  display: block;
  width: 38.4rpx;
  height: 60rpx;
  margin: 6rpx 14rpx;
}

.tabBar-item .title {
  width: 66.4rpx;
  height: 20rpx;
  text-align: center;
  font-size: 20rpx;
  color: #8E8E93;
}

.tabBar .photo {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 34%;
  right: 34%;
}

.tabBar .photo image {
  width: 100%;
  height: 100%;
}

.topView {
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: absolute;
  top: 20rpx;
  left: 32rpx;
  right: 32rpx;
  height: 88rpx;
}

.topView .title {
  font-size: 64rpx;
}

.topView .avatar image {
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
}