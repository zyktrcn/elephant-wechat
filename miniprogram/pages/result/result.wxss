@import "../template/template.wxss";

.container {
  background: #F6FCFF;
}

.banner {
  width: 750rpx;
  height: 500rpx;
}

.result {
  margin-top: 32rpx;
  padding: 0 32rpx;
}

.result .scroller {
  margin-top: 32rpx;
  height: 380rpx;
}

.scroller .item {
  display: inline-block;
  margin-bottom: 32rpx;
  vertical-align: top;
}

.btn {
  margin: 32rpx;
  height: 80rpx;
  border-radius: 32rpx;
  background: #90DEFF;
  text-align: center;
  line-height: 80rpx;
  color: white;
  font-size: 32rpx;
  font-weight: bold;
}
